<%= simple_form_for(@education_class) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :class_name %>
    <%= f.input :class_type %>
    <%= f.input :class_keywords %>
    <%= f.input :class_date_start %>
    <%= f.input :class_date_end %>
    <%= f.input :class_time_start %>
    <%= f.input :class_time_end %>
    <%= f.input :class_worth %>
    <%= f.association :reps %>
    <%= f.input :class_category %>
    <%= f.input :class_url %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
