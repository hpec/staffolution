<%= simple_form_for(@rep) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :rep_name %>
    <%= f.input :rep_type %>
    <%= f.input :rep_description %>
    <%= f.association :user %>
    <%= f.input :rep_email %>
    <%= f.input :rep_phone %>
    <%= f.input :rep_url %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
